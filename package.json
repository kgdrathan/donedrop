{
  "name": "donedrop",
  "version": "1.0.1",
  "description": "Obsidian plugin to auto-sort completed tasks",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "publish": "npm publish"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "sort",
    "tasks",
    "completed",
    "automation"
  ],
  "author": "Gnan Deep Rathan K",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/kgdrathan/donedrop"
  },
  "devDependencies": {
    "@types/node": "^25.0.8",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "builtin-modules": "^5.0.0",
    "esbuild": "^0.27.2",
    "eslint-plugin-obsidianmd": "^0.1.9",
    "obsidian": "^1.11.4",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  }
}
